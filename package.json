{
  "name": "litekart",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-s misiki-copy-config nuxt",
    "nuxt": "nuxt",
    "build": "nuxt build",
    "builda": "nuxt build -a",
    "u": "git pull upstream master",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf public/build && npm install && npm run build:tailwind && npm run build:fontawesome && npm run dev",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:fix": "npm run lint -- --fix",
    "prettify": "prettier --write \"**/*.{js,jsx,vue,json,css,scss,md}\"",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "copy": "node devops/copy",
    "misiki-copy-config": "node devops/misiki/copy-config",
    "anne-copy-config": "node devops/anne/copy-config",
    "anne-copy": "node devops/anne/copy",
    "anne-deploy": "node devops/anne/deploy",
    "anne": "run-s anne-copy-config build anne-copy anne-deploy",
    "anne-deploy-next": "node devops/anne/next",
    "next-anne": "run-s anne-copy-config build anne-copy anne-deploy-next"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "*.{css,vue}": [
      "stylelint",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
